{
  "author": "workpace-technology",
  "name": "workpace-prototypes",
  "version": "1.0.0",
  "description": "knowledge graphs meet notion.",
  "license": "ISC",
  "scripts": {
    "start": "check-engines && npm run start --workspace src",
    "build": "npm run build:src",
    "build:src": "npm run build --workspace src",
    "build:design-system": "npm run build:production --workspace design-system",
    "test": "npm-run-all --parallel test:** --print-label",
    "test:src": "npm run test --workspace src",
    "check": "npm-run-all --parallel check:** --print-label",
    "check:src": "npm run check --workspace src",
    "lint": "npm-run-all --parallel lint:** --print-label",
    "lint:src": "npm run lint --workspace src",
    "format": "npm-run-all --parallel format:* --print-label",
    "format:root": "by-node-env",
    "format:src": "npm run format --workspace src",
    "format:root:development": "prettier --check *.*",
    "format:root:production": "prettier --check *.*",
    "commit": "node scripts/commit.js",
    "sync-droplet": "git pull && npm i && export NODE_ENV=production && npm run build && pm2 reload workpace --update-env && pm2 list",
    "precommit": "npm-run-all precommit:** --print-label",
    "precommit:": "lint-staged",
    "precommit:src": "npm run lint --workspace src",
    "clean": "rm -rf node_modules src/node_modules src/.next design-system/node_modules design-system/dist",
    "dev": "npm run start --workspace src",
    "supabase:init": "supabase init",
    "supabase:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "supabase:diff": "supabase db diff --linked -f",
    "supabase:diff:local": "supabase db diff -f",
    "supabase:update": "npm install -g supabase@latest",
    "supabase:push": "supabase db push --linked",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.1.1",
    "@notionhq/client": "^2.2.15",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^16.4.7",
    "engines": "^0.4.0",
    "ignore": "^5.2.4",
    "jwt-decode": "^3.1.2",
    "openai": "^4.57.0",
    "react-hook-form": "^7.54.2",
    "react-loading": "^2.0.3",
    "react-markdown": "^9.0.1"
  },
  "devDependencies": {
    "by-node-env": "^2.0.1",
    "check-engines": "^1.6.0",
    "fs": "^0.0.1-security",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "notion-types": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.8.7",
    "prompts": "^2.4.2"
  },
  "type": "module",
  "workspaces": [
    "design-system",
    "src"
  ],
  "engines": {
    "node": "^22.0.0",
    "npm": "^9 || ^10"
  }
}
